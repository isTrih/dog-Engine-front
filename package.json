{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 9002",
    "dev:turbo": "next dev --turbopack -p 9002",
    "dev:edge": "EDGE_RUNTIME=edgeone EDGEONE=1 next dev -p 9002",
    "dev:auto": "node scripts/detect-edge-environment.js && npm run dev",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "next build",
    "build:edge": "EDGE_RUNTIME=edgeone EDGEONE=1 next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "edgeone:check": "node scripts/fix-edgeone-compatibility.js check",
    "edgeone:fix": "node scripts/fix-edgeone-compatibility.js fix",
    "edgeone:fix-dry": "node scripts/fix-edgeone-compatibility.js fix --dry-run",
    "edgeone:report": "node scripts/fix-edgeone-compatibility.js report",
    "edgeone:all": "node scripts/fix-edgeone-compatibility.js all",
    "edge:detect": "node scripts/detect-edge-environment.js",
    "edge:auto-deploy": "node scripts/auto-edge-deploy.js",
    "edge:health-check": "node scripts/edge-health-check.js",
    "deploy:edgeone": "npm run build:edge && echo 'EdgeOne部署需要通过腾讯云控制台或CLI完成'",
    "deploy:auto": "npm run edge:detect && npm run edge:auto-deploy",
    "postinstall": "npm run edgeone:check"
  },
  "dependencies": {
    "@genkit-ai/googleai": "^1.14.1",
